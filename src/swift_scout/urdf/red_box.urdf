<?xml version="1.0"?>
<robot name="small_red_box">
  <link name="box_link">
    <visual>
      <geometry>
        <box size="0.1 0.1 0.1"/> <!-- Adjust dimensions if needed -->
      </geometry>
      <material name="RedMaterial">
        <color rgba="1.0 1.0 1.0 1.0"/> 
      </material>
    </visual>
    <collision>
      <geometry>
        <box size="0.1 0.1 0.1"/> <!-- Collision box dimensions matching the visual box -->
      </geometry>
    </collision>
    <inertial>
      <mass value="1"/>
      <inertia ixx="0.1" ixy="0" ixz="0" iyy="0.1" iyz="0" izz="0.1"/>
    </inertial>
  </link>
</robot>
